          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{4FB5D3D8-567E-4C0A-AF31-215FFA0A6519}"
              SQLTask:SqlStatementSource="update comp&#xA;    SET&#xA;        comp.comp_sagecustomercount = ranked.CustomerRecordRank&#xA;FROM&#xA;dbo.Company as comp &#xA;INNER JOIN&#xA;(&#xA;    SELECT &#xA;        cust.Comp_CompanyId,&#xA;        cust.comp_mas_ardivisionno,&#xA;        cust.comp_mas_customerno,&#xA;        RANK() OVER (PARTITION BY cust.comp_mas_ardivisionno, cust.comp_mas_customerno ORDER BY cust.Comp_CompanyId ASC) as CustomerRecordRank&#xA;    FROM&#xA;    dbo.Company as cust&#xA;    WHERE&#xA;    cust.comp_mas_ardivisionno is not NULL&#xA;    and &#xA;    cust.comp_mas_customerno is not null &#xA;) as ranked&#xA;ON&#xA;comp.Comp_CompanyId = ranked.Comp_CompanyId" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
